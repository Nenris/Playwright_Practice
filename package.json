{
  "name": "playwright_tests",
  "version": "1.0.0",
  "description": "Automated Software Testing with Playwright",
  "main": "index.js",
  "scripts": {
    "tests:chrome": "playwright test --config=playwright.config.ts --project=Chromium",
    "tests:firefox": "playwright test --config=playwright.config.ts --project=Firefox",
    "tests:webkit": "playwright test --config=playwright.config.ts --project=Webkit",
    "tests:e2e": "playwright test --config=e2e.config.ts --project=Chromium",
    "tests:visual:chrome": "playwrigth test --config=visual.config.ts --project=Chromium",
    "tests:visual:firefox": "playwrigth test --config=visual.config.ts --project=Firefox",
    "tests:visual:webkit": "playwrigth test --config=visual.config.ts --project=Webkit",
    "tests:visual:chrome:update": "playwrigth test --config=visual.config.ts --project=Chromium --update-snapshots",
    "tests:visual:firefox:update": "playwrigth test --config=visual.config.ts --project=Firefox --update-snapshots",
    "tests:visual:webkit:update": "playwrigth test --config=visual.config.ts --project=Webkit --update-snapshots",
    "tests:api": "playwright test --config=api.config.ts --project=Chromium --reporter=list",
    "jenkins-server": "java -jar jenkins.war --httpPort=8080 --enable-future-java"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nenris/Playwright_Practice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nenris/Playwright_Practice/issues"
  },
  "homepage": "https://github.com/Nenris/Playwright_Practice#readme",
  "dependencies": {
    "@playwright/test": "^1.54.2",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0"
  }
}
